select 
mb.mb_id,
MB.NAME CONTAINER_NAME,
OG.NAME CKT_ID,
CPNY.COMPANY,
mb.desig_id_a,

BLD.DESCRIPTION,
BLD.TEXT BLD_CM,
FL.TEXT Floor,
SUITE.TEXT SUITE_CM,
SUITE.DESCRIPTION SUITE_DESCRIPTION,

COEL.CONT_ID EQUIPMENT_ID,
COMPB.COMPONENT_NAME EQUIPMENT_NAME,
SL.NAME PORT_SLOT,
COMPA.COMPONENT_NAME interfase,
COEL.SLOTS,
COEL.SLOT_NR,
COEL.SERIE,


--
mb.point_id_a,
--POINT.POINT_NAME,
mb.cap_id_a, 
mb.card_id_a,
mb.mux_id_a,
mb.capacity,
mb.unit

from mb_msg_box MB

INNER JOIN OBR_OBJ_REF OB
ON MB.MB_ID =OB.REF_OBJECT_ID --AND OB.REF_CLASS_ID=453 AND MB.FLAG_INTERN=7

INNER JOIN OG_OBJ_GROUP OG
    ON OB.OBJECT_ID=OG.OG_ID
INNER JOIN AHL_ATTACH_LINK OWNER
    ON OG.OG_ID = OWNER.LINK_ID AND ATTACH_KIND = 2  AND ATTACH_TYPE = 12
INNER JOIN CO_COMPANY CPNY
    ON CPNY.COMPANY_ID = OWNER.ATTACH_ID

INNER JOIN DES_DESIGNATION SUITE
ON MB.DESIG_ID_A = SUITE.DESIGNATION_ID

INNER JOIN DES_DESIGNATION FL
ON SUITE.PARENT_ID = FL.DESIGNATION_ID

INNER JOIN DES_DESIGNATION BLD
ON FL.PARENT_ID = BLD.DESIGNATION_ID

INNER JOIN COMPONENT_POINT POINT
ON MB.POINT_ID_A=POINT.POINT_ID

INNER JOIN COMPONENT COMPA
ON POINT.COMPONENT_ID=COMPA.COMPONENT_ID

INNER JOIN coel_cont_elem COEL
ON COMPA.COMPONENT_ID=COEL.ELEM_ID

INNER JOIN COMPONENT COMPB
ON COEL.CONT_ID=COMPB.COMPONENT_ID

INNER JOIN SL_SLOT SL
ON SL.OBJECT_ID = COEL.CONT_ID AND SL.SERIE=COEL.SERIE AND SL.SLOTNR=COEL.SLOT_NR

where mb.name like '%NNI%'
